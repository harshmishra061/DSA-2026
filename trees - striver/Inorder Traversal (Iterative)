class Solution {
public:
    vector<int> inorderTraversal(TreeNode* root) {
        vector<int> res;
        TreeNode* node = root;
        stack<TreeNode*> st;
        while(node != NULL || !st.empty()) {
            if(node != NULL) {
                st.push(node);
                node = node->left;
            } else {
                node = st.top(); st.pop();
                res.push_back(node->val);
                node = node->right;
            }
        }
        return res;
    }
};
