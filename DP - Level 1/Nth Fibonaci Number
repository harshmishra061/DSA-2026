class Solution {
    int fib(int n, vector<int>& dp) {
        if(n == 0 || n == 1) return n;
        if(dp[n] != -1) return dp[n];
        
        int fibnm1 = fib(n - 1, dp); // fib of n - 1
        int fibnm2 = fib(n - 2, dp); // fib of n - 2
        
        int fibn = fibnm1 + fibnm2;
        
        dp[n] = fibn;
        
        return fibn;
    }
  public:
    int nthFibonacci(int n) {
        vector<int> dp(n + 1, -1);
        return fib(n, dp);
    }
};
